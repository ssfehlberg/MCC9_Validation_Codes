# UPS_OVERRIDE is already -B
setenv CETPKG_NAME larsoft
setenv CETPKG_VERSION v06_26_01_14
setenv CETPKG_QUAL e10:prof
setenv CETPKG_TYPE Prof
setenv CETPKG_CC gcc
setenv CETPKG_CXX g++
setenv CETPKG_FC gfortran
set_ setup_fail="false"
setup -B cetbuildtools v7_03_03
test "$?" = 0 || set_ setup_fail="true"
test "$setup_fail" =  "true" && echo "ERROR: setup -B cetbuildtools v7_03_03 failed"
test "$setup_fail" =  "true" && echo "ERROR: Please type \"unsetup cetbuildtools\" and \"unsetup cmake\""
test "$setup_fail" =  "true" && return 1
set_ setup_fail="false"
setup -B cetpkgsupport 
test "$?" = 0 || set_ setup_fail="true"
test "$setup_fail" =  "true" && echo "ERROR: setup -B cetpkgsupport failed"
test "$setup_fail" =  "true" && return 1
# setup products
# Configuring uboonecode
set_ setup_fail="false"
setup -B swtrigger v02_02_03 -q +e10:+prof
test "$?" = 0 || set_ setup_fail="true"
test "$setup_fail" =  "true" && echo "ERROR: setup -B swtrigger v02_02_03 -q +e10:+prof failed"
test "$setup_fail" =  "true" && echo "ERROR: For more information, type \"ups depend swtrigger v02_02_03 -q +e10:+prof\""
test "$setup_fail" =  "true" && echo "                             or \"ups list -aK+ swtrigger v02_02_03\""
test "$setup_fail" =  "true" && return 1
set_ setup_fail="false"
